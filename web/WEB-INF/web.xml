<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>resetpw</servlet-name>
        <servlet-class>ResetPasswordServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>resetpw</servlet-name>
        <url-pattern>/resetPassword</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>course</servlet-name>
        <servlet-class>TCourseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>course</servlet-name>
        <url-pattern>/course</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>class</servlet-name>
        <servlet-class>TClassServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>class</servlet-name>
        <url-pattern>/class</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>homework</servlet-name>
        <servlet-class>THomeworkServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>homework</servlet-name>
        <url-pattern>/homework</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>hwsubmitlistk</servlet-name>
        <servlet-class>HwSubmitListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>hwsubmitlistk</servlet-name>
        <url-pattern>/hw_submit_list</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>hwdetails</servlet-name>
        <servlet-class>THwDetailsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>hwdetails</servlet-name>
        <url-pattern>/hw_details</url-pattern>
    </servlet-mapping>



</web-app>